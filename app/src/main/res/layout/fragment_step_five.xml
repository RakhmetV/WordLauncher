<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.step.five.StepFiveFragment">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayoutFive"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/step_five_exit"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                android:src="@drawable/ic_baseline_arrow_back_24" />

            <ProgressBar
                android:id="@+id/step_five_progress_bar"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="match_parent"

                android:layout_height="10dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="10dp"
                android:progressDrawable="@drawable/custom_progress_step" />

        </LinearLayout>

        <Button
            android:id="@+id/step_five_btn_check"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="Check"
            android:visibility="invisible"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.553"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_btn_res_five" />

        <TextView
            android:id="@+id/step_five_en_one"
            style="@style/step_level_txt_word_en"
            android:layout_marginTop="20dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/step_five_btn_res_one"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutFive" />

        <Button
            android:id="@+id/step_five_btn_res_one"
            style="@style/step_level_btn_res"
            android:layout_width="0dp"
            android:alpha="0"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/step_five_en_one"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/step_five_en_one"
            app:layout_constraintTop_toTopOf="@+id/step_five_en_one" />

        <TextView
            android:id="@+id/step_five_en_two"
            style="@style/step_level_txt_word_en"
            android:layout_marginTop="30dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/step_five_btn_res_two"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_en_one" />

        <Button
            android:id="@+id/step_five_btn_res_two"
            style="@style/step_level_btn_res"
            android:layout_width="0dp"
            android:alpha="0"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/step_five_en_two"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/step_five_en_two"
            app:layout_constraintTop_toTopOf="@+id/step_five_en_two" />

        <TextView
            android:id="@+id/step_five_en_three"
            style="@style/step_level_txt_word_en"
            android:layout_marginTop="30dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/step_five_btn_res_three"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_en_two" />

        <Button
            android:id="@+id/step_five_btn_res_three"
            style="@style/step_level_btn_res"
            android:layout_width="0dp"
            android:alpha="0"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/step_five_en_three"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/step_five_en_three"
            app:layout_constraintTop_toTopOf="@+id/step_five_en_three" />

        <TextView
            android:id="@+id/step_five_en_four"
            style="@style/step_level_txt_word_en"
            android:layout_marginTop="30dp"
            android:alpha="0"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/step_five_btn_res_four"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_en_three" />

        <Button
            android:id="@+id/step_five_btn_res_four"
            style="@style/step_level_btn_res"
            android:layout_width="0dp"
            android:alpha="0"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/step_five_en_four"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/step_five_en_four"
            app:layout_constraintTop_toTopOf="@+id/step_five_en_four" />

        <TextView
            android:id="@+id/step_five_en_five"
            style="@style/step_level_txt_word_en"
            android:layout_marginTop="30dp"
            android:alpha="0"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/step_five_btn_res_five"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_en_four" />

        <Button
            android:id="@+id/step_five_btn_res_five"
            style="@style/step_level_btn_res"
            android:layout_width="0dp"
            android:alpha="0"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/step_five_en_five"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/step_five_en_five"
            app:layout_constraintTop_toTopOf="@+id/step_five_en_five" />

        <LinearLayout
            android:id="@+id/linearLayout16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="150dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/step_five_btn_res_five">

            <Button
                android:id="@+id/step_five_btn_ans_one"
                style="@style/step_level_btn_ans"
                android:text="Button" />

            <Button
                android:id="@+id/step_five_btn_ans_two"
                style="@style/step_level_btn_ans"
                android:text="Button" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutOneBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

            <Button
                android:id="@+id/step_five_ans_seven"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/color_step_answer_button"
                android:text="Button"
                android:textColor="#000000"
                android:textSize="12dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutTwoBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

            <Button
                android:id="@+id/step_five_btn_ans_three"
                style="@style/step_level_btn_ans"
                android:layout_marginStart="4dp"
                android:alpha="0"
                android:text="Button" />

            <Button
                android:id="@+id/step_five_btn_ans_four"
                style="@style/step_level_btn_ans"
                android:alpha="0"
                android:text="Button" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutTwoBtn">

            <Button
                android:id="@+id/step_five_btn_ans_five"
                style="@style/step_level_btn_ans"
                android:alpha="0"
                android:text="Button" />

            <Button
                android:id="@+id/step_five_ans_six"
                style="@style/step_level_btn_ans"
                android:alpha="0"
                android:text="Button" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>

</FrameLayout>